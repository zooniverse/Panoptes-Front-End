    "react-chartist": "~0.10.1",
    "react-component-resizable": "~0.3.0",
    "react-dom": "^0.14.1",
    "react-interpolate-component": "~0.8.0",
    "react-pullout": "~1.0.1",
    "react-router": "~1.0.0",
    "react-select": "~0.9.1",
    "react-swipe": "~3.0.0",
    "react-translate-component": "~0.10.0",
    "remark": "~4.2.2",
    "sort-into-columns": "~1.0.0",
    "strip-markdown": "~0.3.1",
    "sugar-client": "~1.0.1",
    "swipe-js-iso": "~2.0.1",
    "test-shape-closeness": "0.0.1"
  },
  "devDependencies": {
    "babel-cli": "~6.7.7",
    "babel-loader": "~6.2.4",
    "babel-plugin-react-transform": "~2.0.2",
    "babel-plugin-transform-react-jsx": "~6.7.5",
    "babel-preset-es2015": "~6.6.0",
    "babel-preset-stage-0": "~6.5.0",
    "babelify": "~6.4.0",
    "check-engines": "~1.2.0",
    "cjsx-loader": "~3.0.0",
    "coffee-loader": "~0.7.2",
    "coffee-script": "~1.10.0",
    "copy-webpack-plugin": "~2.1.3",
    "css-loader": "~0.23.1",
    "ejs": "~2.3.3",
    "express": "~4.13.3",
    "extract-text-webpack-plugin": "~1.0.1",
    "file-loader": "~0.8.5",
    "html-webpack-plugin": "~2.16.0",
    "json-loader": "~0.5.4",
    "publisssh": "~1.1.0",
    "react-transform-hmr": "~1.0.4",
    "rimraf": "~2.5.2",
    "style-loader": "~0.13.1",
    "stylus": "~0.52.0",
    "stylus-loader": "~2.0.0",
    "webpack": "~1.13.0",
    "webpack-dev-middleware": "~1.6.1",
    "webpack-hot-middleware": "~2.10.0",
    "webpack-split-by-path": "0.0.8"
  },
  "engines": {
    "node": "^4.2.3",
    "npm": "^2.14.7"
  },
  "scripts": {

    "build": "check-engines && webpack --config ./webpack.build.js --progress --profile --colors",
    "clean": "rimraf dist",
    "prep": "export NODE_ENV=staging; npm run clean && npm run build",
    "publish": "publisssh ./dist \"zooniverse-static/${BUCKET}/$DEPLOY_SUBDIR\" $PUBLISH_FLAGS",
    "serve": "export NODE_ENV=staging; check-engines && node server.js",
    "start": "export NODE_ENV=development; check-engines && babel-node server_dev.js",

    "_deploy": "export BUCKET=www.zooniverse.org; npm run clean && npm run build && npm run publish",
    "deploy": "export NODE_ENV=production; export DEPLOY_SUBDIR=panoptes-front-end; export PUBLISH_FLAGS=\"-d\" npm run _deploy",

    "_stage": "export BUCKET=preview.zooniverse.org; npm run clean && npm run build && npm run publish",
    "stage": "export NODE_ENV=staging; export DEPLOY_SUBDIR=panoptes-front-end; npm run _stage",
    "stage-branch": "export NODE_ENV=staging; export BRANCH_SUBDIR=$(git symbolic-ref --short HEAD); export DEPLOY_SUBDIR=panoptes-front-end/${BRANCH_SUBDIR}; export PUBLISH_FLAGS=\"-r\"; npm run _stage; echo \"Link: https://${BRANCH_SUBDIR}.pfe-preview.zooniverse.org/\"",

    "stage-with-docker": "./bin/run-through-docker.sh 'npm run-script stage'",
    "stage-branch-with-docker": "./bin/run-through-docker.sh 'npm run-script stage-branch'",

    "test": "echo 'Integration tests have moved to the panoptes-client library.'"
  }
}
