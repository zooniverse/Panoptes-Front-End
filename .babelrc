{
  "presets": ["es2015", "stage-0"],
  "env": {
    "development": {
      "plugins": [
        ["react-transform", {
          "transforms": [{
            "transform": "react-transform-hmr",
            "imports": ["react"],
            "locals": ["module"]
          }]
        }],
        ["transform-react-jsx"]
      ]
    },
    "staging": {
      "plugins": [
        ["transform-react-jsx"]
      ]
    },
    "production": {
      "plugins": [
        ["transform-react-jsx"]
      ]
    }
  }
}
