.paginator
  text-align: center

  .paginator-page-selector
    display: inline-block
    margin: 0 10px

  .paginator-prev
    background: none
    color: FOREGROUND
    &:hover
      background: FAILURE
      color: #fff

  .paginator-next
    background: none
    color: FOREGROUND
    &:hover
      background: SUCCESS
      color: #fff